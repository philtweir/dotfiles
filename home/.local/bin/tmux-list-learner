#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'tmux-list-learner','console_scripts','tmux-list-learner'
__requires__ = 'tmux-list-learner'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('tmux-list-learner', 'console_scripts', 'tmux-list-learner')()
    )
